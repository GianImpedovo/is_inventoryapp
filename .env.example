
# Example .env
# For AWS RDS, your provider often gives you something like:
# postgres://<USER>:<PASS>@<HOST>:5432/<DBNAME>
# If your RDS enforces TLS, leave PGSSL as default (true). For local dev, set PGSSL=false
DATABASE_URL=postgres://postgres:postgres@localhost:5432/inventory
PGSSL=true
PORT=80